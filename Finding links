//we have to find all links which contain python

let a='python';
let links=document.links;
console.log(links);
let href;
Array.from(links).forEach(function(element){    //  this give all link present in web page
    href=element.href;
    console.log(href);
});



Array.from(links).forEach(function(element){                      // this gives only those links which contains python
    href=element.href;
    if(href.includes(a)){
    console.log(href);}
});
